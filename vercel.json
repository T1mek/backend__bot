{
   "version": 2,
   "name":"backend__bot",
   "builds": [
       {
           "src": "src/main.js",
           "use": "@vercel/node"
       }
   ],
   "routes": [
       {
           "src": "/(.*)",
           "dest": "src/main.js",
           "methods": [
               "GET",
               "POST",
               "PUT",
               "DELETE"
           ]
       }
   ]
}